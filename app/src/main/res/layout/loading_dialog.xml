<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/viewLoading"
    android:layout_width="@dimen/progress_dialog_height_width"
    android:layout_height="@dimen/progress_dialog_height_width"
    android:background="@drawable/progress_dialog_bg"
    android:gravity="center"
    android:layoutDirection="ltr">

    <com.app.utils.NestedProgress
        android:id="@+id/progress_view"
        android:layout_width="@dimen/progress_dialog_height_width"
        android:layout_height="@dimen/progress_dialog_height_width"
        app:innerAnimInterpolator="overshoot"
        app:innerLoaderColor="@color/color_inner"
        app:innerLoaderStrokeWidth="3dp"
        app:outerAnimInterpolator="linear"
        app:outerLoaderAnimDuration="1500"
        app:outerLoaderColor="@color/color_outer"
        app:outerLoaderStrokeWidth="2dp" />


</RelativeLayout>